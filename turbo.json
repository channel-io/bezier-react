{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/next-v1",
  "pipeline": {
    "build": {
      "outputs": [
        "dist/**"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "build:ci": {
      "outputs": [
        "dist/**"
      ],
      "dependsOn": [
        "^build:ci"
      ]
    },
    "test": {
      "outputs": [
        "coverage/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "typecheck": {
      "outputs": []
    },
    "jest": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "release": {
      "outputs": []
    }
  }
}
