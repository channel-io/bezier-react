{
  "transition": {
    "timing-function": {
      "default": {
        "value": [0.3, 0, 0, 1],
        "type": "cubicBezier"
      }
    },
    "duration": {
      "s": {
        "value": "150ms",
        "type": "duration"
      },
      "m": {
        "value": "300ms",
        "type": "duration"
      },
      "l": {
        "value": "450ms",
        "type": "duration"
      }
    },
    "s": {
      "value": {
        "duration": "{transition.duration.s}",
        "timingFunction": "{transition.timing-function.default}"
      },
      "type": "transition"
    },
    "m": {
      "value": {
        "duration": "{transition.duration.m}",
        "timingFunction": "{transition.timing-function.default}"
      },
      "type": "transition"
    },
    "l": {
      "value": {
        "duration": "{transition.duration.l}",
        "timingFunction": "{transition.timing-function.default}"
      },
      "type": "transition"
    }
  }
}
