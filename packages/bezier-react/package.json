{
  "name": "@channel.io/bezier-react",
  "version": "1.0.0-next-v1.135",
  "description": "React components from Bezier Design System",
  "keywords": [
    "design",
    "react",
    "channel"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/channel-io/bezier-react"
  },
  "license": "ISC",
  "author": "Channel Corp.",
  "sideEffects": false,
  "main": "dist/index.cjs.js",
  "module": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "prebuild": "npm run clean:build",
    "build": "cross-env BABEL_ENV=build rollup -c",
    "build:storybook": "build-storybook",
    "clean": "rm -rf node_modules .turbo && npm run clean:build",
    "clean:build": "rm -rf dist",
    "deploy:storybook": "storybook-to-ghpages --remote=upstream",
    "dev": "npm run storybook",
    "eslint-all": "eslint 'src/**/*.+(js|ts)?(x)'",
    "jest": "cross-env BABEL_ENV=test jest --maxWorkers=2",
    "jest:watch": "cross-env BABEL_ENV=test jest --watch",
    "lint": "npm run eslint-all && npm run stylelint-all",
    "prepublishOnly": "npm run build",
    "release": "npm run semantic-release",
    "semantic-release": "semantic-release",
    "storybook": "start-storybook -p 4101",
    "stylelint-all": "stylelint src/**/*.styled.{js,ts}",
    "test": "npm run lint && npm run typecheck && npm run jest",
    "typecheck": "ts-prune -e -p ./tsconfig.prune.json && tsc",
    "update-snapshot": "jest --updateSnapshot"
  },
  "browserslist": [
    "ie >= 11",
    "> 1% in KR",
    "> 1% in JP",
    "> 1% in US"
  ],
  "dependencies": {
    "@babel/runtime": "^7.12.13",
    "@channel.io/bezier-react-icons": "^1.0.0",
    "lodash-es": "^4.17.15",
    "react-resize-detector": "^7.1.1",
    "react-textarea-autosize": "^8.3.4",
    "ssr-window": "^3.0.0",
    "typesafe-actions": "^5.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@mdx-js/react": "^1.6.22",
    "@storybook/addon-a11y": "^6.5.0",
    "@storybook/addon-actions": "^6.5.0",
    "@storybook/addon-backgrounds": "^6.5.0",
    "@storybook/addon-controls": "^6.5.0",
    "@storybook/addon-docs": "^6.5.0",
    "@storybook/addon-toolbars": "^6.5.0",
    "@storybook/react": "^6.5.0",
    "@storybook/storybook-deployer": "^2.8.11",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^13.1.1",
    "@testing-library/user-event": "^14.1.1",
    "@types/jest": "^25.2.3",
    "@types/lodash-es": "^4.17.4",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@types/styled-components": "^5.1.5",
    "@types/uuid": "^8.3.0",
    "jest": "^26.6.3",
    "jest-styled-components": "^7.0.4",
    "paths.macro": "^3.0.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "styled-components": "^5.3.5",
    "stylelint": "^13.8.0",
    "ts-prune": "^0.10.3",
    "tsconfig": "*"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "styled-components": ">=5"
  }
}
