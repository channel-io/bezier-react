{
  "name": "@channel.io/bezier-react",
  "version": "1.0.0-next-v1.180",
  "description": "React components library that implements Bezier Design System.",
  "repository": {
    "type": "git",
    "url": "https://github.com/channel-io/bezier-react"
  },
  "main": "build/index.cjs.js",
  "module": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "sideEffects": false,
  "files": [
    "build",
    "src"
  ],
  "scripts": {
    "build": "run-s 'build:*'",
    "build:icon": "./scripts/build-icon.sh",
    "build:rollup": "cross-env BABEL_ENV=production rollup -c",
    "dev": "yarn storybook",
    "lint": "run-p 'lint:*'",
    "lint:js": "TIMING=1 eslint --cache 'src/**/*.+(js|ts)?(x)'",
    "lint:style": "stylelint 'src/**/*.styled.{js,ts}' --cache",
    "typecheck": "yarn find-deadcode && tsc --build --verbose",
    "find-deadcode": "ts-prune -e -p ./tsconfig.prune.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "update-snapshot": "jest --updateSnapshot",
    "clean": "rm -rf node_modules .turbo & yarn clean:build",
    "clean:build": "npx rimraf build",
    "prebuild": "yarn clean:build",
    "storybook": "start-storybook -p 4101",
    "build-storybook": "build-storybook",
    "chromatic": "chromatic --project-token=06157a6fbe6f"
  },
  "keywords": [
    "design",
    "react",
    "channel"
  ],
  "author": "Channel Corp.",
  "license": "Apache-2.0",
  "browserslist": [
    "> 1% in KR",
    "> 1% in JP",
    "> 1% in US",
    "not ie <= 11"
