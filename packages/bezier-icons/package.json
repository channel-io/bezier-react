{
  "name": "@channel.io/bezier-icons",
  "version": "0.1.0",
  "description": "Icons the make up the Bezier design system.",
  "repository": {
    "type": "git",
    "url": "https://github.com/channel-io/bezier-react",
    "directory": "packages/bezier-icons"
  },
  "main": "index.js",
  "sideEffects": false,
  "scripts": {
    "build": "yarn generate",
    "generate": "run-p 'generate:*'",
    "generate:react": "./scripts/react/generate-icon.sh",
    "lint": "TIMING=1 eslint --cache .",
    "clean": "run-s 'clean:*'",
    "clean:build": "rm -rf dist",
    "clean:cache": "rm -rf node_modules .turbo .eslintcache tsconfig.tsbuildinfo coverage"
  },
  "keywords": [
    "channel",
    "design",
    "svg",
    "icons"
  ],
  "author": "Channel Corp.",
  "license": "Apache-2.0",
  "devDependencies": {
    "@svgr/babel-plugin-add-jsx-attribute": "^8.0.0",
    "@svgr/babel-plugin-remove-jsx-attribute": "^8.0.0",
    "@svgr/cli": "^8.0.1",
    "@types/react": "^18.2.6",
    "eslint-config-bezier": "workspace:^",
    "tsconfig": "workspace:^"
  }
}
