{
  "name": "@channel.io/bezier-icons",
  "version": "0.1.0",
  "description": "Icons the make up the Bezier design system.",
  "repository": {
    "type": "git",
    "url": "https://github.com/channel-io/bezier-react",
    "directory": "packages/bezier-icons"
  },
  "main": "dist/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c",
    "optimize": "svgo -f ./icons -o ./dist",
    "generate": "run-p 'generate:*'",
    "generate:react": "./scripts/react/generate-icon.sh",
    "lint": "TIMING=1 eslint --cache .",
    "clean": "run-s 'clean:*'",
    "clean:build": "rm -rf dist",
    "clean:cache": "rm -rf node_modules .turbo .eslintcache tsconfig.tsbuildinfo coverage"
  },
  "keywords": [
    "channel",
    "design",
    "svg",
    "icons"
  ],
  "author": "Channel Corp.",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-virtual": "^3.0.1",
    "@rollup/pluginutils": "^5.0.2",
    "@svgr/cli": "^8.0.1",
    "@svgr/core": "^8.0.0",
    "eslint-config-bezier": "workspace:^",
    "rollup": "^3.22.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "svgo": "^3.0.2",
    "tsconfig": "workspace:^"
  }
}
